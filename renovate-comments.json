{
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "{{arg0}}",
        "{{arg1}}",
        "{{arg2}}",
        "{{arg3}}",
        "{{arg4}}",
        "{{arg5}}",
        "{{arg6}}",
        "{{arg7}}",
        "{{arg8}}",
        "{{arg9}}"
      ],
      "matchStrings": [
        "(?<currentValue>[\\w+\\.\\-]*(@(?<currentDigest>sha256:\\w{64}))?)['\",;]*\\s*(#|\\\/\\\/)\\s?renovate: (?<datasource>\\S+)=(?<depName>\\S+)\\s?(registry=(?<registryUrl>\\S+))?\\s?(versioning=(?<versioning>\\S+))?\\s?(extractVersion=(?<extractVersion>\\S+))?"
      ]
    }
  ]
}
